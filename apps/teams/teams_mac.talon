os: mac
app: Microsoft Teams
-

settings():
    user.mouse_continuous_scroll_amount = 300


channel: key(super-e)
go to left section: key(ctrl-l)

open helpdesk: key(super-6)
open inside gartner: key(super-7)
open onedrive: key(super-8)

file upload:
    key(shift-alt-o)
    sleep(100ms)
    key(up)
    key(space)

go back: key(super-[)
forward: key(super-])

hunt this: key(super-f)
